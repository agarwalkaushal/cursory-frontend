{
	"name": "Cursory",
	"version": "1.0",
	"description": "Crisp highlights & summary of any article!",
	"permissions": [
		"activeTab",
		"tabs",
		"https://cursory-nlp.herokuapp.com/*",
		"*://*/*",
		"<all_urls>"
	],
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "images/get_started16.png",
			"32": "images/get_started32.png",
			"48": "images/get_started48.png",
			"128": "images/get_started128.png"
		}
	},
	"background": {
		"scripts": ["background.js"]
	},
	"icons": {
		"16": "images/get_started16.png",
		"32": "images/get_started32.png",
		"48": "images/get_started48.png",
		"128": "images/get_started128.png"
	},
	// "content_scripts": [
	// 	{
	// 		"matches": [
	// 			"<all_urls>"
	// 		],
	// 		"js": [
	// 			"background.js"
	// 		]
	// 	}
	// ],
	"manifest_version": 2
}
